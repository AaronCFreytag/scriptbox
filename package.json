{
  "name": "scriptbox",
  "version": "0.0.1",
  "description": "A server for the sandbox programming platformer Scriptbox",
  "private": true,
  "main": "main.ts",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest --verbose false",
    "test-coverage": "jest --verbose false --coverage ",
    "watch-build": "tsc --p \"./src\" -w",
    "watch-copy": "cpx -w \"./scripts/**/*\" dist/__scripted__",
    "watch-test": "jest --watch",
    "watch": "npm-run-all --parallel watch-build watch-copy",
    "build": "tsc --p \"./src\" && npm run copy-scripts",
    "copy-scripts": "cpx \"./scripts/**/*\" dist/__scripted__",
    "start": "node dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "http://www.github.com/scriptbox-team/scriptbox"
  },
  "author": "Aaron Freytag",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/supertest": "^2.0.8",
    "cpx": "^1.5.0",
    "jest": "^24.8.0",
    "mockdate": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typedoc": "^0.15.0"
  },
  "dependencies": {
    "@types/express": "^4.17.1",
    "@types/express-fileupload": "^1.1.0",
    "@types/fs-extra": "^8.0.0",
    "@types/lodash": "^4.14.137",
    "@types/module-alias": "^2.0.0",
    "@types/mongodb": "^3.3.3",
    "@types/ws": "^6.0.1",
    "browser-process-hrtime": "^1.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "file-type": "^12.2.0",
    "fs-extra": "^8.1.0",
    "isolated-vm": "^1.7.10",
    "lodash": "^4.17.15",
    "module-alias": "^2.2.0",
    "mongodb": "^3.3.2",
    "source-map-support": "^0.5.12",
    "supertest": "^4.0.2",
    "tsc": "^1.20150623.0",
    "typescript": "^3.5.3",
    "ws": "^6.2.1"
  }
}
